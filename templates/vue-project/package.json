{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "yarn g:clean",
    "start": "yarn g:app-start",
    "preview": "yarn g:app-preview",
    "build": "yarn g:build",
    "build:watch": "yarn g:build-watch",
    "lint": "yarn g:lint",
    "fix": "yarn g:fix",
    "check": "yarn g:app-check",
    "test": "yarn g:test",
    "test:watch": "yarn g:test-watch",
    "coverage": "yarn g:coverage"
  },
  "prettier": "prettier-config-custom",
  "eslintConfig": {
    "extends": [
      "vue"
    ],
    "parserOptions": {
      "project": [
        "tsconfig.json",
        "tsconfig.node.json"
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "pinia": "^2.1.7",
    "radix-vue": "^1.1.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "eslint-config-vue": "workspace:^",
    "prettier-config-custom": "workspace:^",
    "tsconfig-custom": "workspace:^",
    "vite-config-custom": "workspace:^"
  }
}
